@model IEnumerable<OnlineStore.Common.DTOs.OrderDto>

<table class="table">
    <thead>
        <tr>
            <th>Order ID</th>
            <th>Total Price</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.Id</td>
                <td>@order.TotalPrice</td>
                <td>
                    <a href="@Url.Action("OrderDetails", "Order", new { Id = order.Id })" class="btn btn-primary">Details</a>
                </td>
            </tr>

        }
    </tbody>
</table>